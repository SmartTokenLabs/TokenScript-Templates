{
	"$schema": "../tstemplate.schema.json",
	"name": "Empty Svelte Project - recommended",
	"description": "An empty Svelte project",
	"templateFields": [
		{
			"name": "TokenScript name",
			"token": "TOKENSCRIPT_NAME",
			"prompt": "Enter a name for the TokenScript project",
			"value": "smart token layer"
		},
		{
			"name": "Token type",
			"token": "TOKEN_TYPE",
			"updatePrefix": "interface=\"",
			"prompt": "Select the token type",
			"options": [
				"erc721",
				"erc20",
				"erc1155"
			],
			"value": "erc20"
		},
		{
			"name": "Contract address",
			"token": "CONTRACT_ADDRESS",
			"prompt": "Enter the ethereum contract address for the TokenScript",
			"value": "0xdb82c0d91e057e05600c8f8dc836beb41da6df14"
		},
		{
			"name": "Contract chain",
			"token": "CONTRACT_CHAIN",
			"updatePrefix": "network=\"",
			"prompt": "Enter the ethereum chainID for the TokenScript",
			"value": "1"
		},
		{
			"name": "Description",
			"token": "DESCRIPTION",
			"prompt": "Enter a short description for the TokenScript",
			"value": "smart token layer official tokenscript"
		},
		{
			"name": "About URL",
			"token": "ABOUT_URL",
			"prompt": "Enter a URL to a website about this TokenScript",
			"value": "https://www.smartlayer.network/"
		},
		{
			"name": "Icon URL",
			"token": "ICON_URL",
			"prompt": "Enter an icon URL for the TokenScript (base64 image URLs are supported)",
			"value": "data:image/png;base64,"
		},
		{
			"name": "Trimmed Name",
			"token": "TOKENSCRIPT_TRIM",
			"prompt": "",
			"value": "smart-token-layer"
		}
	]
}