				<ts:viewContent name="common"/>
			</ts:view>
		</ts:card>
		<ts:card buttonClass="secondary" name="Buy" origins="Token" type="token">
			<ts:label>
				<ts:string xml:lang="en">Buy</ts:string>
			</ts:label>
			<ts:view xmlns="http://www.w3.org/1999/xhtml" urlFragment="buy" xml:lang="en">
				<ts:viewContent name="common"/>
			</ts:view>
		</ts:card>
		<ts:card buttonClass="secondary" name="Bridge" origins="Token" type="token">
			<ts:label>
				<ts:string xml:lang="en">Bridge</ts:string>
			</ts:label>
			<ts:view xmlns="http://www.w3.org/1999/xhtml" urlFragment="bridge" xml:lang="en">
				<ts:viewContent name="common"/>
