		<ts:card name="Info" origins="Token" type="token">
			<ts:label>
				<ts:string xml:lang="en">Info</ts:string>
			</ts:label>
			<ts:view xmlns="http://www.w3.org/1999/xhtml" urlFragment="info" xml:lang="en">
				<ts:viewContent name="common"/>
			</ts:view>
		</ts:card>
		<ts:card name="Send" origins="Token" type="action">
			<ts:label>
				<ts:string xml:lang="en">Send</ts:string>
			</ts:label>
			<ts:transaction>
				<ethereum:transaction as="uint" contract="Token" function="transfer">
					<ts:data>
						<ts:address ref="receivingAccountAddress"/>
						<ts:uint256 ref="receivingAmount"/>
					</ts:data>
				</ethereum:transaction>
			</ts:transaction>
			<ts:view xmlns="http://www.w3.org/1999/xhtml" urlFragment="send" xml:lang="en">
				<ts:viewContent name="common"/>
			</ts:view>
		</ts:card>
	</ts:cards>

	<ts:attribute name="sendingAccountAddress">
		<ts:type>
			<ts:syntax>1.3.6.1.4.1.1466.115.121.1.15</ts:syntax>
		</ts:type>
		<ts:label>
			<ts:string xml:lang="en">sending account address</ts:string>
		</ts:label>
		<ts:origins>
			<ts:user-entry as="address"/>
		</ts:origins>
	</ts:attribute>
	
	<ts:attribute name="receivingAccountAddress">
		<ts:type>
			<ts:syntax>1.3.6.1.4.1.1466.115.121.1.15</ts:syntax>
		</ts:type>
		<ts:label>
			<ts:string xml:lang="en">receiving account address</ts:string>
		</ts:label>
		<ts:origins>
			<ts:user-entry as="address"/>
		</ts:origins>
	</ts:attribute>

	<ts:attribute name="receivingAmount">
		<ts:type>
			<ts:syntax>1.3.6.1.4.1.1466.115.121.1.15</ts:syntax>
		</ts:type>
		<ts:label>
			<ts:string xml:lang="en">receiving amount</ts:string>
		</ts:label>
		<ts:origins>
			<ts:user-entry as="uint"/>
		</ts:origins>
	</ts:attribute>
	
