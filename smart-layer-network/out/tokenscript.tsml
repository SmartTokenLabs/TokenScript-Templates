* {
	font-family: "Rubik", sans-serif;
	box-sizing: border-box;
}

body {
	color: white;
	background-color: black;
}

#token-container {
	margin-bottom: 44px;
}

div, p {
	word-break: break-word;
}

.no-button-style {
	border: none;
	cursor: pointer;
	background: none;
	text-decoration: underline;
}

h1 {
	font-size: 22px;
}

h2 {
	font-size: 20px;
}

h3 {
	font-size: 18px;
}

h5 {
	font-size: 16px;
	margin-bottom: 12px;
}

p.light {
	color: #545454;
}

.loader-modal {
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
}

.flex-center {
	display: flex;
	justify-content: center;
}

.flex-between {
	display: flex;
	justify-content: space-between;
}

.field-section {
	margin: 18px 18px 38px 18px;
}

.field-section-heading {
	margin: 24px 0;
}

.field-section-title {
	font-size: 24px; font-weight: 500;
}

.field-icon {
	width: 27px;
	height: 27px;
	font-size: 19px;
	text-align: center;
	font-weight: 500;
}

.field-container {
	margin: 16px 0;
}
.field-title {
	margin-bottom: 9px;
}

.field-value {
	color: #BDFF02
} 

.max-button {
	min-width: 28px;
}
