<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ts:token xmlns:ts="http://tokenscript.org/2022/09/tokenscript"
		  xmlns:xml="http://www.w3.org/XML/1998/namespace"
		  xsi:schemaLocation="http://tokenscript.org/2022/09/tokenscript https://www.tokenscript.org/schemas/2022-09/tokenscript.xsd"
		  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		  xmlns:ethereum="urn:ethereum:constantinople"
		  name="ENS-1155">
	<ts:label>
		<ts:plurals xml:lang="en">
			<ts:string quantity="one">ENS-1155 Token</ts:string>
			<ts:string quantity="other">ENS-1155 Tokens</ts:string>
		</ts:plurals>
	</ts:label>
	<ts:meta>
		<ts:description xml:lang="en">ENS 1155 template</ts:description>
		<ts:aboutUrl xml:lang="en">https://app.ens.domains/</ts:aboutUrl>
		<ts:iconUrl xml:lang="en">https://app.ens.domains///image.jpg</ts:iconUrl>
	</ts:meta>
	<ts:contract interface="erc1155" name="Token">
		<!-- <ts:address network="1">0xd4416b13d2b3a9abae7acd5d6c2bbdbe25686401</ts:address> -->
		<!-- <ts:address network="11155111">0x0635513f179D50A207757E05759CbD106d7dFcE8</ts:address> -->
		<ts:address network="1">0xd4416b13d2b3a9abae7acd5d6c2bbdbe25686401</ts:address>
	</ts:contract>
	<ts:contract name="ETHRegistrarController">
		<!-- <ts:address network="1">0x253553366Da8546fC250F225fe3d25d0C782303b</ts:address> -->
		<!-- <ts:address network="11155111">0xFED6a969AaA60E4961FCD3EBF1A2e8913ac65B72</ts:address> -->
		<ts:address network="1">0x253553366Da8546fC250F225fe3d25d0C782303b</ts:address>
	</ts:contract>
	<ts:contract name="PublicResolver">
		<!-- <ts:address network="1">0x231b0Ee14048e9dCcD1d247744d114a4EB5E8E63</ts:address> -->
		<!-- <ts:address network="11155111">0x8FADE66B79cC9f707aB26799354482EB93a5B7dD</ts:address> -->
		<ts:address network="1">0x231b0Ee14048e9dCcD1d247744d114a4EB5E8E63</ts:address>
	</ts:contract>
	<ts:origins>
		<!-- Define the contract which holds the token that the user will use -->
		<ts:ethereum contract="Token"/>
	</ts:origins>

	<ts:cards>
		<ts:viewContent name="common" xmlns="http://www.w3.org/1999/xhtml">
			<ts:include type="html" src="./dist/index.html"/>
			<ts:include type="css" src="./src/styles.css"/>
		</ts:viewContent>

		<ts:card type="token" name="Info" origins="Token">
			<ts:label>
				<ts:string xml:lang="en">Info</ts:string>
			</ts:label>
			<ts:view xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" urlFragment="info">
				<ts:viewContent name="common"/>
			</ts:view>
		</ts:card>

		<ts:card type="action" name="Renew">
			<ts:label>
				<ts:string xml:lang="en">Renew</ts:string>
			</ts:label>

			<ts:attribute name="renewalSeconds">
				<ts:type>
					<ts:syntax>1.3.6.1.4.1.1466.115.121.1.27</ts:syntax>
				</ts:type>
				<ts:origins>
					<ts:user-entry as="uint"/>
				</ts:origins>
			</ts:attribute>

			<ts:attribute name="baseENSTokenId">
				<ts:type>
					<ts:syntax>1.3.6.1.4.1.1466.115.121.1.27</ts:syntax>
				</ts:type>
				<ts:origins>
					<ts:user-entry as="uint"/>
				</ts:origins>
			</ts:attribute>

			<ts:transaction>
				<ethereum:transaction function="renew" contract="ETHRegistrarController" as="uint">
					<ts:data>
						<ts:uint256 ref="baseENSTokenId"/>
						<ts:uint256 ref="renewalSeconds"/>
					</ts:data>
					<ethereum:value ref="renewalPrice" />
				</ethereum:transaction>
			</ts:transaction>

			<ts:view xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" urlFragment="renew">
				<ts:viewContent name="common"/>
			</ts:view>
		</ts:card>

		<ts:card type="action" name="Record">
			<ts:label>
				<ts:string xml:lang="en">Update</ts:string>
			</ts:label>

			<ts:attribute name="newRecordValue">
				<ts:type>
					<ts:syntax>1.3.6.1.4.1.1466.115.121.1.15</ts:syntax>
				</ts:type>
				<ts:label>
					<ts:string xml:lang="en">new record value</ts:string>
				</ts:label>
				<ts:origins>
					<ts:user-entry as="utf8"/>
				</ts:origins>
			</ts:attribute>

			<ts:attribute name="newRecordKey">
				<ts:type>
					<ts:syntax>1.3.6.1.4.1.1466.115.121.1.15</ts:syntax>
				</ts:type>
				<ts:label>
					<ts:string xml:lang="en">new record key</ts:string>
				</ts:label>
				<ts:origins>
					<ts:user-entry as="utf8"/>
				</ts:origins>
			</ts:attribute>

			<ts:attribute name="email">
				<ts:type>
					<ts:syntax>1.3.6.1.4.1.1466.115.121.1.15</ts:syntax>
				</ts:type>
				<ts:label>
					<ts:string xml:lang="en">email</ts:string>
				</ts:label>
				<ts:origins>
					<ts:user-entry as="utf8"/>
				</ts:origins>
			</ts:attribute>

			<ts:attribute name="location">
				<ts:type>
					<ts:syntax>1.3.6.1.4.1.1466.115.121.1.15</ts:syntax>
				</ts:type>
				<ts:label>
					<ts:string xml:lang="en">location</ts:string>
				</ts:label>
				<ts:origins>
					<ts:user-entry as="utf8"/>
				</ts:origins>
			</ts:attribute>

			<ts:attribute name="notice">
				<ts:type>
					<ts:syntax>1.3.6.1.4.1.1466.115.121.1.15</ts:syntax>
				</ts:type>
				<ts:label>
					<ts:string xml:lang="en">notice</ts:string>
				</ts:label>
				<ts:origins>
					<ts:user-entry as="utf8"/>
				</ts:origins>
			</ts:attribute>

			<ts:attribute name="display">
				<ts:type>
					<ts:syntax>1.3.6.1.4.1.1466.115.121.1.15</ts:syntax>
				</ts:type>
				<ts:label>
					<ts:string xml:lang="en">display</ts:string>
				</ts:label>
				<ts:origins>
					<ts:user-entry as="utf8"/>
				</ts:origins>
			</ts:attribute>

			<ts:attribute name="url">
				<ts:type>
					<ts:syntax>1.3.6.1.4.1.1466.115.121.1.15</ts:syntax>
				</ts:type>
				<ts:label>
					<ts:string xml:lang="en">url</ts:string>
				</ts:label>
				<ts:origins>
					<ts:user-entry as="utf8"/>
				</ts:origins>
			</ts:attribute>

			<ts:attribute name="phone">
				<ts:type>
					<ts:syntax>1.3.6.1.4.1.1466.115.121.1.15</ts:syntax>
				</ts:type>
				<ts:label>
					<ts:string xml:lang="en">phone</ts:string>
				</ts:label>
				<ts:origins>
					<ts:user-entry as="utf8"/>
				</ts:origins>
			</ts:attribute>

			<ts:attribute name="avatar">
				<ts:type>
					<ts:syntax>1.3.6.1.4.1.1466.115.121.1.15</ts:syntax>
				</ts:type>
				<ts:label>
					<ts:string xml:lang="en">avatar</ts:string>
				</ts:label>
				<ts:origins>
					<ts:user-entry as="utf8"/>
				</ts:origins>
			</ts:attribute>

			<ts:attribute name="keywords">
				<ts:type>
					<ts:syntax>1.3.6.1.4.1.1466.115.121.1.15</ts:syntax>
				</ts:type>
				<ts:label>
					<ts:string xml:lang="en">keywords</ts:string>
				</ts:label>
				<ts:origins>
					<ts:user-entry as="utf8"/>
				</ts:origins>
			</ts:attribute>

			<ts:attribute name="description">
				<ts:type>
					<ts:syntax>1.3.6.1.4.1.1466.115.121.1.15</ts:syntax>
				</ts:type>
				<ts:label>
					<ts:string xml:lang="en">avatar</ts:string>
				</ts:label>
				<ts:origins>
					<ts:user-entry as="utf8"/>
				</ts:origins>
			</ts:attribute>

			<ts:transaction>
				<ethereum:transaction function="setText" contract="PublicResolver" as="bool">
					<ts:data>
						<ts:bytes32 ref="tokenIdHex"/>
						<ts:string ref="newRecordKey"/>
						<ts:string ref="newRecordValue"/>
					</ts:data>
				</ethereum:transaction>
			</ts:transaction>

			<ts:view xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" urlFragment="record">
				<ts:viewContent name="common"/>
			</ts:view>
		</ts:card>

	</ts:cards>

	<ts:attribute name="tokenIdHex">
		<ts:type>
			<ts:syntax>1.3.6.1.4.1.1466.115.121.1.15</ts:syntax>
		</ts:type>
		<ts:label>
			<ts:string xml:lang="en">tokenIdHex</ts:string>
		</ts:label>
		<ts:origins>
			<ts:user-entry as="uint"/>
		</ts:origins>
	</ts:attribute>

</ts:token>
